{
    "name": "Note It",
    "version": "1.0",
    "description": "Be able to highlight items on a page and add notes.",
    "manifest_version": 2,
    "background": {
        "scripts": ["client/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "client/popup.html"
    },
    "icons": {
        "16": "client/assets/highlightIcon.png",
        "48": "client/assets/highlightIcon.png",
        "128": "client/assets/highlightIcon.png"
    },

    

    "permissions":[
        "storage",
        "<all_urls>",
        "identity"
    ],

    "oauth2": {
        "client_id": "616174675838-lhonhh213amr4hl1brlcq6ooa03h35lu.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/plus.me"
        ]
    }
}